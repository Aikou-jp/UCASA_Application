<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ucasa_application">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ServiceActivity"
            android:label="@string/title_activity_service"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".RuleActivity"
            android:label="@string/title_activity_rule"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SensorOptionActivity"
            android:label="@string/title_activity_sensor_option"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SensorActivity"
            android:label="@string/title_activity_sensor"
            android:parentActivityName=".SelectActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SelectActivity"
            android:label="@string/title_activity_select">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>